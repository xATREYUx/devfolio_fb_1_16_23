{
  "presets": [
    "@babel/preset-env",
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "plugins": [
    ["@babel/plugin-transform-react-jsx", { "throwIfNamespace": false }]
  ],
  //removes yellow warning re The code generator has deoptimised the styling of .js as it exceeds the max of 500KB.
  "env": {
    "development": {
      "compact": false
    }
  }
}
